  "description": "Padma Yoga API Project",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node dist",
    "dev": "NODE_ENV=development nodemon --exec babel-node src/index.js",
    "build": "babel src -d dist",
    "lint": "eslint --ext .js --ignore-path .gitignore ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/padma-yoga/padma-yoga-api.git"
  },
  "author": "Rodrigo Avellar de Muniagurria",
  "homepage": "https://github.com/padma-yoga/padma-yoga-api#readme",
  "dependencies": {
    "@sendgrid/mail": "^7.1.0",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.2.1",
    "mongoose": "^5.9.10",
    "prettier": "^2.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.0",
    "debug": "^4.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "nodemon": "^2.0.3"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "git add"
    ],
    "*.js": [
      "eslint",
      "git add",
      "jest --bail --findRelatedTests"
